{
  "name": "dex-price-estimator",
  "version": "0.0.1",
  "description": "A service estimating feasible sell prices for Gnosis Protocol",
  "main": "build/index.js",
  "scripts": {
    "build": "rimraf build && tsc",
    "start": "node .",
    "verify": "yarn lint && yarn install --frozen-lockfile",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint --fix src --ext .ts,.js",
    "test": "jest"
  },
  "keywords": [
    "Gnosis",
    "Protocol",
    "price",
    "estimation",
    "dfusion"
  ],
  "author": "Gnosis",
  "husky": {
    "hooks": {
      "pre-commit": "yarn verify"
    }
  },
  "dependencies": {
    "@gnosis.pm/dex-contracts": "^0.2.6",
    "@types/yargs": "^15.0.4",
    "express": "^4.17.1",
    "typescript-logging": "^0.6.4",
    "web3": "^1.2.6",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "jest": "^25.2.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  }
}

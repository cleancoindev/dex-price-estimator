{
  "name": "dex-price-estimator",
  "version": "0.0.4",
  "description": "A service estimating feasible sell prices for Gnosis Protocol",
  "main": "build/index.js",
  "scripts": {
    "bench": "autocannon -c 5 -d 30 'http://localhost:8080/api/v1/markets/1-7/estimated-buy-amount/1000000000000000000?atoms=true'",
    "build": "rimraf build && tsc",
    "prestart": "yarn build",
    "start": "node .",
    "start-no-build": "node .",
    "verify": "yarn lint && yarn install --frozen-lockfile",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint --fix src --ext .ts,.js",
    "test": "jest"
  },
  "keywords": [
    "Gnosis",
    "Protocol",
    "price",
    "estimation",
    "dfusion"
  ],
  "author": "Gnosis",
  "husky": {
    "hooks": {
      "pre-commit": "yarn verify"
    }
  },
  "dependencies": {
    "@anxolin/express-prom-bundle": "^6.0.0",
    "@gnosis.pm/dex-contracts": "^0.2.11",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "prom-client": "^12.0.0",
    "typescript-logging": "^0.6.4",
    "web3": "^1.2.7",
    "workerpool": "5.0.4",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.9.3",
    "@types/supertest": "^2.0.9",
    "@types/workerpool": "^5.0.1",
    "@types/yargs": "^15.0.4",
    "@typescript-eslint/parser": "^2.31.0",
    "autocannon": "^4.6.0",
    "eslint": "^7.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "typescript": "^3.8.3"
  }
}
